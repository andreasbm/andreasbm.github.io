{
	"name": "andreas",
	"version": "1.0.0",
	"scripts": {
		"clean:dist": "rimraf dist",
		"start": "vite",
		"build": "npm run clean-dist && npx vite build --manifest --outDir dist/ --assetsDir src/ && npm run copy-assets",
		"clean-dist": "rm -rf dist",
		"copy-assets": "cp -rv assets/ dist/assets"
	},
	"dependencies": {
		"lit": "^3.1.1",
		"web-dialog": "^0.0.11"
	},
	"devDependencies": {
		"vite": "^5.0.11"
	}
}
