{
	"name": "andreas",
	"version": "1.0.0",
	"scripts": {
		"serve": "http-server --push-state -o -c-1 -p 1200 -a localhost",
		"s": "npm run serve",
		"b": "NODE_ENV=production parcel build index.html",
		"deploy": "npm run b && git subtree push --prefix dist origin gh-pages",
		"snowpack": "npx snowpack --optimize",
		"postinstall": "npm run snowpack"
	},
	"dependencies": {
		"lit-element": "^2.2.1",
		"web-dialog": "0.0.8"
	},
	"devDependencies": {
		"parcel-bundler": "^1.12.4",
		"snowpack": "^1.4.0"
	},
	"snowpack": {
		"dedupe": [
			"lit-html"
		],
		"webDependencies": [
			"lit-html",
			"lit-element",
			"web-dialog",
			"lit-html/directives/repeat.js"
		]
	}
}
